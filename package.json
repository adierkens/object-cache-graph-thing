{
  "name": "object-cache-graph-thing",
  "version": "0.1.0",
  "description": "A thing for making object traversals quicker",
  "main": "dist/main.js",
  "module": "dist/main.module.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean build:*",
    "build:commonjs": "cross-env NODE_ENV=commonjs babel -d dist src --ignore /'__tests__'/",
    "build:module": "babel ./src/main.js -o ./dist/main.module.js",
    "test": "cross-env NODE_ENV=test nyc ava",
    "test:watch": "ava --watch",
    "posttest": "npm run lint",
    "lint": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adierkens/object-cache-graph-thing.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "tree",
    "traversal",
    "object",
    "cache"
  ],
  "author": "Adam Dierkens <adam@dierkens.com>",
  "license": "Beerware",
  "bugs": {
    "url": "https://github.com/adierkens/object-cache-graph-thing/issues"
  },
  "homepage": "https://github.com/adierkens/object-cache-graph-thing#readme",
  "dependencies": {
    "es6-map": "~0.1.5",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "ava": "~0.20.0",
    "babel-cli": "~6.24.1",
    "babel-plugin-istanbul": "~4.1.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "~1.5.2",
    "babel-preset-es2015": "6.24.1",
    "babel-require": "~1.0.1",
    "babel-runtime": "~6.23.0",
    "cross-env": "~5.0.1",
    "npm-run-all": "~4.0.2",
    "nyc": "~11.0.3",
    "rimraf": "~2.6.1",
    "xo": "~0.18.2"
  },
  "xo": {
    "space": true
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "source": [
      "**/*.js",
      "!__tests__/**/*"
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "html",
      "cobertura"
    ],
    "sourceMap": false,
    "instrument": "false"
  },
  "babel": {
    "plugins": [
      [
        "transform-runtime",
        "transform-object-rest-spread"
      ]
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4,
            "browsers": [
              "ie >= 10",
              "Edge >= 12",
              "Firefox >= 11",
              "Chrome >= 4",
              "Opera >= 30",
              "iOS >= 5",
              "Android >= 4.0"
            ],
            "uglify": true
          },
          "modules": false
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ],
        "presets": [
          [
            "env",
            {
              "modules": "commonjs"
            }
          ]
        ]
      },
      "commonjs": {
        "presets": [
          [
            "env",
            {
              "modules": "commonjs"
            }
          ]
        ]
      }
    }
  }
}
